{
  "name": "expect-mcp",
  "version": "0.0.1",
  "description": "Custom Vitest matchers for testing Model Context Protocol (MCP) integrations.",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@8.15.4",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./vitest": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    },
    "./vitest-setup": {
      "types": "./dist/vitest.d.ts",
      "default": "./dist/vitest.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "vitest",
    "expect",
    "matchers",
    "mcp",
    "model context protocol",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "sideEffects": false,
  "peerDependencies": {
    "vitest": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.4.0",
    "vitest": "^1.5.0"
  },
  "dependencies": {
    "@facetlayer/json-rpc-subprocess": "file:../node-libraries/json-rpc-subprocess"
  }
}
